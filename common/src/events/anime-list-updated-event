import { Subjects } from "./subjects";

export interface AnimeListUpdatedEvent {
  subject: Subjects.AnimeListUpdated;
  data: {
    titles: {
        title: string;
        type: string;
    }[];
    type: string;
    malId: string;
    images: string;
    episodes: number;
    duration: string;
    rating: string;
    score: number;
    synopsis: string;
    genres: {
        name: string
    }[];
  };
}